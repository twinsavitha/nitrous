<%= form_for :article, url: addings_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.label :role %><br>
    <%= f.text_area :role %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>